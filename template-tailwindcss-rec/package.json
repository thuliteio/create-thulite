    "build": "hugo --minify --gc",
    "preview": "vite preview --outDir public"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "@thulite/images": "^3.3.1",
    "@thulite/seo": "^2.4.1",
    "@thulite/tailwindcss": "^1.3.1",
    "thulite": "^2.6.2"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "vite": "^7.0.5"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
