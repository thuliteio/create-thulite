{
  "name": "tailwindcss-starter-rec",
  "version": "0.0.0",
  "description": "Thulite Tailwind CSS starter with recommended integrations",
  "author": "Thulite",
  "license": "MIT",
  "scripts": {
    "create": "hugo new",
    "dev": "hugo server --disableFastRender --noHTTPCache",
