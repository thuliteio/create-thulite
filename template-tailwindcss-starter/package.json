{
  "name": "tailwindcss-starter",
  "version": "0.0.0",
  "description": "Thulite Tailwind CSS starter with recommended integrations",
  "author": "Thulite",
  "license": "MIT",
  "scripts": {
    "create": "hugo new",
    "dev": "hugo server --disableFastRender --noHTTPCache",
    "format": "prettier **/** -w -c",
    "build": "hugo --minify --gc",
    "preview": "vite preview --outDir public"
  },
  "dependencies": {
    "@thulite/tailwindcss": "^1.2.0",
    "thulite": "^2.5.0"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "vite": "^5.4.2"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
