{
  "name": "tailwindcss-starter",
  "version": "0.0.0",
  "description": "Thulite Tailwind CSS starter",
  "author": "Thulite",
  "license": "MIT",
  "scripts": {
    "create": "hugo new",
    "dev": "hugo server --disableFastRender --noHTTPCache",
    "format": "prettier **/** -w -c",
    "build": "hugo --minify --gc",
    "preview": "vite preview --outDir public"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@thulite/tailwindcss": "^1.3.2",
    "thulite": "^2.6.4"
  },
  "devDependencies": {
    "prettier": "^3.8.1",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": ">=24.13.0"
  }
}
